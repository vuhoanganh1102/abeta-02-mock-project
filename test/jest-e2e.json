{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/send-mail-ha/(.*)": "<rootDir>/../libs/send-mail-ha/src/$1",
    "@app/send-mail-ha": "<rootDir>/../libs/send-mail-ha/src",
    "@app/sendgrid/(.*)": "<rootDir>/../libs/sendgrid/src/$1",
    "@app/sendgrid": "<rootDir>/../libs/sendgrid/src"
  }
}